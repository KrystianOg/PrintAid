# Development Environment Overrides
NODE_ENV=development
LOG_LEVEL=debug

# Development Database (can use different DB for dev)
# POSTGRES_DB=medusa_dev

# Development CORS - more permissive
STORE_CORS=http://localhost:3000,http://localhost:8000,http://127.0.0.1:3000
ADMIN_CORS=http://localhost:7001,http://localhost:9000,http://127.0.0.1:7001

# DATABASE_URL=${DATABASE_URL}?sslmodel=disable

# Development Redis - smaller memory limit
REDIS_MAX_MEMORY=128mb

# Enable development features
MEDUSA_ADMIN_ONBOARDING_TYPE=default
MEDUSA_DISABLE_TELEMETRY=true

# Hot reload and development settings
CHOKIDAR_USEPOLLING=true
